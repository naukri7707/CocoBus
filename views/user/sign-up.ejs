<div class="row">
  <div class="col-12">
    <div id="sign-up">
      <div class="field">
        <div>*帳號</div>
        <input id="username" type="text" placeholder="帳號">
      </div>
      <div class="field">
        <div>*密碼</div>
        <input id="password" type="password" placeholder="密碼">
      </div>
      <div class="field">
        <div>*確認密碼</div>
        <input id="checkPassword" type="password" placeholder="確認密碼">
      </div>
      <div class="field">
        <div>暱稱</div>
        <input id="nickname" type="text" placeholder="暱稱">
      </div>
      <div class="field">
        <div>手機號碼</div>
        <input id="cellphone" type="text" placeholder="手機號碼">
      </div>
      <div class="field">
        <div>電子信箱</div>
        <input id="email" type="text" placeholder="電子信箱">
      </div>
      <button class=" btn btn-primary" onclick="signup()">註冊</button>
    </div>
  </div>
</div>